<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto" >

    <TextView
        android:id="@+id/date_id"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/id_desawar"
        android:background="@drawable/table_content_cell_bg"
        android:text="Date"
        android:gravity="center_horizontal"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/id_desawar"
        android:layout_width="57dp"
        app:layout_constraintTop_toTopOf="parent"
        android:gravity="center_horizontal"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@+id/date_id"
        app:layout_constraintRight_toLeftOf="@+id/id_faridabad"
        android:background="@drawable/table_content_cell_bg"
        android:text="88"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/id_faridabad"
        android:layout_width="57dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        android:gravity="center_horizontal"
        app:layout_constraintLeft_toRightOf="@+id/id_desawar"
        app:layout_constraintRight_toLeftOf="@+id/id_gaziabad"
        android:background="@drawable/table_content_cell_bg"
        android:text="99"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/id_gaziabad"
        android:layout_width="57dp"
        app:layout_constraintTop_toTopOf="parent"
        android:gravity="center_horizontal"
        app:layout_constraintLeft_toRightOf="@+id/id_faridabad"
        app:layout_constraintRight_toLeftOf="@+id/id_gali"
        android:layout_height="wrap_content"
        android:background="@drawable/table_content_cell_bg"
        android:text="33"
        android:textAlignment="center"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/id_gali"
        android:layout_width="57dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        android:gravity="center_horizontal"
        app:layout_constraintLeft_toRightOf="@+id/id_gaziabad"
        app:layout_constraintRight_toLeftOf="@+id/id_rewari"
        android:background="@drawable/table_content_cell_bg"
        android:text="33"
        android:textAlignment="center"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/id_rewari"
        android:layout_width="57dp"
        app:layout_constraintTop_toTopOf="parent"
        android:gravity="center_horizontal"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@+id/id_gali"
        app:layout_constraintRight_toRightOf="parent"
        android:background="@drawable/table_content_cell_bg"
        android:text="33"
        android:textAlignment="center"
        android:textSize="14sp" />

</androidx.constraintlayout.widget.ConstraintLayout>